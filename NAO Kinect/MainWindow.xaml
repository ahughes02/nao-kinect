        <Label Content="NAO - DISCONNECTED" HorizontalAlignment="Left" Margin="726,56,0,0" VerticalAlignment="Top" Height="41" Width="326" FontSize="24"/>
        <Label Content="Kinect - DISCONNECTED" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="41" Width="326" FontSize="24"/>
        <Slider x:Name="lsrSlider" HorizontalAlignment="Left" Margin="160,577,0,0" VerticalAlignment="Top" Width="183" Maximum="1.33" Minimum="-0.32" IsEnabled="False"/>
        <Slider x:Name="rerSlider" HorizontalAlignment="Left" Margin="160,683,0,0" VerticalAlignment="Top" Width="183" IsEnabled="False" Maximum="-0.034" Minimum="-1.55"/>
        <Slider x:Name="lerSlider" HorizontalAlignment="Left" Margin="160,648,0,0" VerticalAlignment="Top" Width="183" Minimum="-1.55" Maximum="-0.034" IsEnabled="False"/>
        <Slider x:Name="rsrSlider" HorizontalAlignment="Left" Margin="160,613,0,0" VerticalAlignment="Top" Width="183" IsEnabled="False" Maximum="1.33" Minimum="-0.32"/>
