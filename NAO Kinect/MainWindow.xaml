        <Image x:Name="Image" HorizontalAlignment="Left" Height="480" Margin="10,56,0,0" VerticalAlignment="Top" Width="640" Source="NO FEED.png"/>
        <TextBlock x:Name="debug" HorizontalAlignment="Left" Height="117" Margin="731,418,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="405" FontSize="28" Text="READY"/>
