        <Label x:Name="Debug_Label_Copy" Content="LSR:" HorizontalAlignment="Left" Height="31" Margin="10,568,0,0" VerticalAlignment="Top" Width="119" FontSize="16"/>
        <Label x:Name="Debug_Label_Copy1" Content="RER:" HorizontalAlignment="Left" Height="31" Margin="10,675,0,0" VerticalAlignment="Top" Width="119" FontSize="16"/>
        <Label x:Name="Debug_Label_Copy2" Content="LER:" HorizontalAlignment="Left" Height="31" Margin="10,640,0,0" VerticalAlignment="Top" Width="119" FontSize="16"/>
        <Label x:Name="Debug_Label_Copy3" Content="RSR:" HorizontalAlignment="Left" Height="31" Margin="10,604,0,0" VerticalAlignment="Top" Width="119" FontSize="16"/>
        <Label x:Name="Debug_Label_Copy4" Content="LSP" HorizontalAlignment="Left" Height="31" Margin="380,706,0,0" VerticalAlignment="Top" Width="40" FontSize="16"/>
        <Label x:Name="Debug_Label_Copy5" Content="RSP" HorizontalAlignment="Left" Height="31" Margin="445,706,0,0" VerticalAlignment="Top" Width="40" FontSize="16"/>

    </Grid>
</Window>
